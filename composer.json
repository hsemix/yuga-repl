{
    "name": "yuga/repl",
    "description": "Interact with your Yuga Application.",
    "keywords": ["framework", "Yuga", "Repl", "PHP Repl"],
    "license": "MIT",
    "homepage": "https://yuga-framework.gitbook.io/documentation/repl",
    "authors": [
        {
            "name": "Hamidouh Semix",
            "email": "semix.hamidouh@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.1",
        "psy/psysh": "^0.10.4|^0.11.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "yuga/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Yuga\\Repl\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "yuga": {
            "providers": [
                "Yuga\\Repl\\Providers\\ReplServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
